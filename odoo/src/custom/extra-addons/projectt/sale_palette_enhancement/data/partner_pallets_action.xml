<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="action_recompute_pallets_server" model="ir.actions.server">
        <field name="name">Recalculer Palettes</field>
        <field name="model_id" ref="base.model_res_partner"/>
        <field name="binding_model_id" ref="base.model_res_partner"/>
        <field name="binding_view_types">list</field>
        <field name="state">code</field>
        <field name="code">
            for partner in records:
            partner.action_recompute_pallets()
        </field>
    </record>

    <record id="action_set_printable_server" model="ir.actions.server">
        <field name="name">Autorise Impression</field>
        <field name="model_id" ref="base.model_res_partner"/>
        <field name="binding_model_id" ref="base.model_res_partner"/>
        <field name="binding_view_types">list</field>
        <field name="state">code</field>
        <field name="code">
            for partner in records:
            partner.action_set_printable()
        </field>
    </record>

</odoo>